{
  "name": "@wuhu/app",
  "exports": "./server.ts",
  "compilerOptions": {
    "lib": ["deno.window", "dom"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "rootDirs": [".", "./.react-router/types"],
    "types": ["vite/client"],
    "strict": true
  },
  "tasks": {
    "dev": "deno run -A @wuhu/react-router-deno/dev",
    "build": "deno run -A @wuhu/react-router-deno/build",
    "start": "deno run -A server.ts",
    "typegen": "deno run -A npm:@react-router/dev typegen",
    "check": {
      "command": "deno check '**/*.ts' '**/*.tsx'",
      "dependencies": ["typegen"]
    }
  },
  "exclude": ["build/"],
  "imports": {
    "@mariozechner/pi-agent-core": "npm:@mariozechner/pi-agent-core@^0.52.7",
    "@mariozechner/pi-ai": "npm:@mariozechner/pi-ai@^0.52.7",
    "@std/assert": "jsr:@std/assert@^1",
    "react-markdown": "npm:react-markdown@^10.1.0",
    "remark-gfm": "npm:remark-gfm@^4.0.1",
    "~/": "./app/",
    "@react-router/dev": "npm:@react-router/dev@^7.13",
    "@types/react": "npm:@types/react@^19",
    "isbot": "npm:isbot@^5",
    "react": "npm:react@19",
    "react-dom": "npm:react-dom@19",
    "react-router": "npm:react-router@^7.13",
    "vite": "npm:vite@^7",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4",
    "tailwindcss": "npm:tailwindcss@^4"
  }
}
