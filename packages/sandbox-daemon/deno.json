{
  "name": "@wuhu/sandbox-daemon",
  "exports": {
    ".": "./mod.ts",
    "./types.ts": "./types.ts"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4",
    "@hono/hono/streaming": "jsr:@hono/hono@^4/streaming",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/path": "jsr:@std/path@^1",
    "zod": "npm:zod@4.3.6"
  },
  "tasks": {
    "check": "deno check . && deno lint",
    "test": "deno test -A",
    "coverage": "deno test -A --coverage=coverage && deno coverage coverage --detailed",
    "coverage:lcov": "deno test -A --coverage=coverage && deno coverage coverage --lcov --output=coverage.lcov",
    "coverage:check": "deno run -A ./scripts/coverage_check.ts"
  }
}
